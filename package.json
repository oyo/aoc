{
  "name": "aoc.stuff",
  "version": "0.0.1",
  "description": "AoC Code",
  "main": "index.js",
  "author": "Martin Rohrmeier <rohrmeier@web.de>",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "requirements-check": "node src/util/check-version.js",
    "use-latest": "export PATH=$(echo $PATH | sed -e 's/node@14/node@17/')",
    "test": "jest",
    "all": "node src/run",
    "solve": "node src/util/solve",
    "start": "yarn run open",
    "open": "concurrently \"cd resources/html && http-server -a localhost -p 1234\" \"open http://localhost:1234/\"",
    "deploy": "aws s3 sync --acl public-read resources/html/ s3://rohrmeier/aoc"
  },
  "dependencies": {
    "https-proxy-agent": "^5.0.0",
    "jimp": "^0.16.1",
    "lodash": "^4.17.21",
    "lodash.product": "^18.9.19",
    "node-fetch": "^2.6.1",
    "tesseract.js": "^2.1.5"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "http-server": "^13.0.2",
    "jest": "^27.2.4",
    "semver": "^7.3.5"
  }
}
